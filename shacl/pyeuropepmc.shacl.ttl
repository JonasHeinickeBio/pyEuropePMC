bibo:Document a sh:NodeShape ;
    sh:closed false ;
    sh:description "Base entity for scholarly works (papers, references, etc.). Provides common fields and methods for entities representing scholarly publications." ;
    sh:ignoredProperties ( pyeuropepmc:citationCount bibo:pageStart dcterms:related pyeuropepmc:affiliatedWith bibo:pageEnd cito:isCitedBy dcterms:abstract datacite:openalex dcterms:creator dcterms:hasPart bibo:DocumentType dcterms:accessRights dcterms:publisher bibo:numPages bibo:issn bibo:journal pyeuropepmc:semanticScholarCorpusId rdf:type dcterms:rights bibo:issue cito:cites pyeuropepmc:influentialCitationCount dcterms:license foaf:fundedBy dcterms:subject dcterms:description bibo:uri ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Semantic Scholar paper ID" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
            sh:path pyeuropepmc:semanticScholarId ],
        [ sh:datatype xsd:float ;
            sh:description "Confidence score for extracted information (0.0 to 1.0)" ;
            sh:maxCount 1 ;
            sh:maxInclusive 1e+00 ;
            sh:minInclusive 0e+00 ;
            sh:nodeKind sh:Literal ;
            sh:order 12 ;
            sh:path pyeuropepmc:confidence ],
        [ sh:datatype xsd:integer ;
            sh:description "Publication year (4-digit)" ;
            sh:maxCount 1 ;
            sh:maxInclusive 9999 ;
            sh:minInclusive 1000 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path dcterms:issued ],
        [ sh:datatype xsd:string ;
            sh:description "PubMed Central ID" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path pyeuropepmc:pmcid ;
            sh:pattern "^PMC\\d+$" ],
        [ sh:datatype xsd:anyURI ;
            sh:description "RDF types (CURIEs/URIs) for this entity" ;
            sh:nodeKind sh:Literal ;
            sh:order 13 ;
            sh:path rdf:type ],
        [ sh:datatype xsd:string ;
            sh:description "Local identifier (slug/uuid) for the entity" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 9 ;
            sh:path dcterms:identifier ],
        [ sh:datatype xsd:anyURI ;
            sh:description "Digital Object Identifier" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path bibo:doi ;
            sh:pattern "^10\\.\\d{4,9}/[-._;()/:A-Z0-9]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "List of data sources that contributed to this entity" ;
            sh:nodeKind sh:Literal ;
            sh:order 14 ;
            sh:path prov:hadPrimarySource ],
        [ sh:datatype xsd:string ;
            sh:description "PubMed ID" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path pyeuropepmc:pmid ;
            sh:pattern "^\\d+$" ],
        [ sh:datatype xsd:string ;
            sh:description "Page range (e.g., \"123-456\")" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path bibo:pages ],
        [ sh:datatype xsd:string ;
            sh:description "Work or entity title" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path dcterms:title ],
        [ sh:datatype xsd:date ;
            sh:description "Full publication date" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path dcterms:date ],
        [ sh:datatype xsd:dateTime ;
            sh:description "Timestamp of last data update (ISO 8601)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 15 ;
            sh:path dcterms:modified ],
        [ sh:datatype xsd:anyURI ;
            sh:description "Source URI for provenance tracking" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 11 ;
            sh:path prov:hadPrimarySource ],
        [ sh:datatype xsd:string ;
            sh:description "Publication volume" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path bibo:volume ],
        [ sh:datatype xsd:string ;
            sh:description "Human-readable label for the entity" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 10 ;
            sh:path rdfs:label ] ;
    sh:targetClass bibo:Document .

pyeuropepmc:BaseEntity a sh:NodeShape ;
    sh:closed false ;
    sh:description "Base entity for all data models with RDF serialization support. All entities inherit from this base class, providing common functionality for validation, normalization, and RDF export." ;
    sh:ignoredProperties ( pyeuropepmc:rorId skos:prefLabel geo1:lat nif:endIndex dcterms:language bibo:pageStart nif:isString pyeuropepmc:semanticScholarId dcterms:related foaf:mbox bibo:pageEnd owl:sameAs cito:isCitedBy foaf:givenName geo1:countryCode pyeuropepmc:pmid bibo:nlmid dcterms:issued dcterms:abstract datacite:openalex dcterms:creator bibo:nlmTA frapo:hasRecipient bibo:shortTitle bibo:doi pyeuropepmc:isni bibo:DocumentType dcterms:accessRights dcterms:publisher pyeuropepmc:impactFactor pyeuropepmc:publisherId foaf:made geo1:long frapo:funds foaf:familyName bibo:issn dcterms:created bibo:journal org:classification org:role dcterms:title bibo:pages pyeuropepmc:semanticScholarCorpusId dcterms:rights bibo:issue cito:cites dcterms:license foaf:initials geo1:city foaf:depiction bibo:eissn pyeuropepmc:sjr bibo:article pyeuropepmc:citationCount nif:beginIndex dcterms:date pyeuropepmc:affiliatedWith bibo:uri org:hasMember datacite:identifier dcterms:hasPart pyeuropepmc:gridId bibo:volume dcterms:isPartOf rdfs:member pyeuropepmc:hIndex foaf:name datacite:doi bibo:numPages foaf:homepage frapo:hasFundingAgency datacite:orcid geo1:country rdf:type pyeuropepmc:fundrefId pyeuropepmc:influentialCitationCount bibo:isoAbbrev foaf:fundedBy dcterms:subject dcterms:description pyeuropepmc:affiliationText pyeuropepmc:pmcid pyeuropepmc:scopusSourceId ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Human-readable label for the entity" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path rdfs:label ],
        [ sh:datatype xsd:string ;
            sh:description "List of data sources that contributed to this entity" ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path prov:hadPrimarySource ],
        [ sh:datatype xsd:anyURI ;
            sh:description "Source URI for provenance tracking" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path prov:hadPrimarySource ],
        [ sh:datatype xsd:float ;
            sh:description "Confidence score for extracted information (0.0 to 1.0)" ;
            sh:maxCount 1 ;
            sh:maxInclusive 1e+00 ;
            sh:minInclusive 0e+00 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path pyeuropepmc:confidence ],
        [ sh:datatype xsd:string ;
            sh:description "Local identifier (slug/uuid) for the entity" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path dcterms:identifier ],
        [ sh:datatype xsd:anyURI ;
            sh:description "RDF types (CURIEs/URIs) for this entity" ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path rdf:type ],
        [ sh:datatype xsd:dateTime ;
            sh:description "Timestamp of last data update (ISO 8601)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path dcterms:modified ] ;
    sh:targetClass pyeuropepmc:BaseEntity .

frapo:Grant a sh:NodeShape ;
    sh:closed true ;
