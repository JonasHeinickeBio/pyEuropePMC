@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix pyeuropepmc: <https://w3id.org/pyeuropepmc/vocab#> .

# PyEuropePMC Custom Vocabulary Ontology
# Defines properties specific to biomedical literature and knowledge graphs
# that are not adequately covered by existing ontologies

pyeuropepmc: a owl:Ontology ;
    rdfs:label "PyEuropePMC Vocabulary"@en ;
    rdfs:comment "Custom vocabulary for PyEuropePMC RDF mappings"@en ;
    owl:versionInfo "1.0.0"@en .

# Identifier Properties
pyeuropepmc:semanticScholarId a owl:DatatypeProperty ;
    rdfs:label "Semantic Scholar ID"@en ;
    rdfs:comment "Identifier for a work in Semantic Scholar"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:string .

pyeuropepmc:openAlexId a owl:DatatypeProperty ;
    rdfs:label "OpenAlex ID"@en ;
    rdfs:comment "Identifier for a work in OpenAlex"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:string .

pyeuropepmc:semanticScholarCorpusId a owl:DatatypeProperty ;
    rdfs:label "Semantic Scholar Corpus ID"@en ;
    rdfs:comment "Corpus identifier in Semantic Scholar"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:integer .

pyeuropepmc:openAlexAuthorId a owl:DatatypeProperty ;
    rdfs:label "OpenAlex Author ID"@en ;
    rdfs:comment "Author identifier in OpenAlex"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:string .

pyeuropepmc:openAlexInstitutionId a owl:DatatypeProperty ;
    rdfs:label "OpenAlex Institution ID"@en ;
    rdfs:comment "Institution identifier in OpenAlex"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:string .

# Citation and Impact Metrics
pyeuropepmc:citationCount a owl:DatatypeProperty ;
    rdfs:label "Citation Count"@en ;
    rdfs:comment "Total number of citations for a work"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:integer .

pyeuropepmc:influentialCitationCount a owl:DatatypeProperty ;
    rdfs:label "Influential Citation Count"@en ;
    rdfs:comment "Number of influential citations for a work"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:integer .

pyeuropepmc:impactFactor a owl:DatatypeProperty ;
    rdfs:label "Impact Factor"@en ;
    rdfs:comment "Journal impact factor"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:decimal .

pyeuropepmc:sjr a owl:DatatypeProperty ;
    rdfs:label "SCImago Journal Rank"@en ;
    rdfs:comment "SCImago Journal Rank indicator"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:decimal .

pyeuropepmc:hIndex a owl:DatatypeProperty ;
    rdfs:label "h-index"@en ;
    rdfs:comment "h-index metric for journals or authors"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:integer .

