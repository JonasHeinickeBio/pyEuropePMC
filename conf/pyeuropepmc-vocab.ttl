# Relationship Properties
pyeuropepmc:hasAuthor a owl:ObjectProperty ;
    rdfs:label "has author"@en ;
    rdfs:comment "Relates a work to its author"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range owl:Thing .

